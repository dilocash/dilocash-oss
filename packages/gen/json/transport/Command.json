{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Command",
    "definitions": {
        "Command": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "UUID generated in the client (localdb primary key)"
                },
                "created_at": {
                    "type": "string",
                    "description": "critical fields for offline-first synchronization  Timestamp in milliseconds (our Checkpoint)",
                    "format": "date-time"
                },
                "updated_at": {
                    "type": "string",
                    "description": "Timestamp in milliseconds (our Checkpoint)",
                    "format": "date-time"
                },
                "deleted": {
                    "type": "boolean",
                    "description": "Flag for \"Soft Deletes\" (localdb requires it)"
                },
                "command_status": {
                    "enum": [
                        "COMMAND_STATUS_UNSPECIFIED",
                        0,
                        "COMMAND_STATUS_PENDING",
                        1,
                        "COMMAND_STATUS_PROCESSING",
                        2,
                        "COMMAND_STATUS_FAILED",
                        3,
                        "COMMAND_STATUS_SYNCED",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Command Status"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Command",
            "description": "represents the command of a financial transaction"
        }
    }
}