{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/IntentsList",
    "definitions": {
        "IntentsList": {
            "properties": {
                "created": {
                    "items": {
                        "$ref": "#/definitions/dilocash.v1.Intent"
                    },
                    "type": "array"
                },
                "updated": {
                    "items": {
                        "$ref": "#/definitions/dilocash.v1.Intent"
                    },
                    "type": "array"
                },
                "deleted": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Intents List"
        },
        "dilocash.v1.Intent": {
            "properties": {
                "id": {
                    "type": "string",
                    "description": "UUID generated in the client (localdb primary key)"
                },
                "text_message": {
                    "type": "string"
                },
                "audio_message": {
                    "type": "string"
                },
                "image_message": {
                    "type": "string"
                },
                "intent_status": {
                    "enum": [
                        "INTENT_STATUS_UNSPECIFIED",
                        0,
                        "INTENT_STATUS_PROCESSING",
                        1,
                        "INTENT_STATUS_PENDING_REVIEW",
                        2,
                        "INTENT_STATUS_CONFIRMED",
                        3,
                        "INTENT_STATUS_FAILED",
                        4
                    ],
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "integer"
                        }
                    ],
                    "title": "Intent Status"
                },
                "requires_review": {
                    "type": "boolean"
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Intent",
            "description": "represents the intent of a financial transaction"
        }
    }
}